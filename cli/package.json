  },
  "keywords": [
    "mastra",
    "ai",
    "agents",
    "registry",
    "typescript",
    "javascript",
    "openai",
    "workflows",
    "tools",
    "community",
    "opensource",
    "shadcn",
    "automation",
    "prompt-engineering",
    "sharing",
    "reusable"
  ],
  "author": "Aidalinfo",
  "module": "index.ts",
  "type": "module",
  "devDependencies": {
    "@types/bun": "latest",
    "@types/yargs": "^17.0.33"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "yargs": "^18.0.0"
  }
}
